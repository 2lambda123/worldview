{
    "eos": {
        "query": {
            "timeWindow": [-180, 180]    
        },
        "filters": [
            "extent",
            "time"
        ],
        "options": {
            "extent": {
                "4326": [-180, -60, 180, 60]
            },
            "time": {
                "meridian": 720,
                "eastMixZone": [-180, 180],
                "westMixZone": [1260, 1620]
            }
        }
    }
}
