{
    "eos": {
        "query": {
            "timeWindow": [-180, 180]    
        },
        "filters": [
            "extent",
            "time"
        ],
        "options": {
            "extent": {
                "4326": [-180, -60, 180, 60]
            },
            "time": {
                "maxDistance": 270,
                "eastZone": 120,
                "westZone": 1200
            }
        }
    }
}
