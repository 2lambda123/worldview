<?xml version="1.0"?>
<StyledLayerDescriptor xmlns="http://www.opengis.net/sld" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd">
  <NamedLayer>
    <Name>ndh-cyclone-hazard-frequency-distribution</Name>
    <UserStyle>
      <Name>SEDAC</Name>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <ColorMap>
	      <ColorMapEntry color="#000000" opacity="0" label="No Cyclones"/>
              <ColorMapEntry color="#FFFFD4" label="1 - 5"/>
	      <ColorMapEntry color="#FED98E" label="6 - 15"/>
	      <ColorMapEntry color="#FE9929" label="16 - 30"/>
	      <ColorMapEntry color="#CC4C02" label="31 - 65"/>
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

