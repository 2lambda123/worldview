<?xml version="1.0"?>
<StyledLayerDescriptor xmlns="http://www.opengis.net/sld" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd">
  <NamedLayer>
    <Name>grump-v1-population-count_2000</Name>
    <UserStyle>
      <Name>SEDAC</Name>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <ColorMap>
	      <ColorMapEntry color="#000000" opacity="0" label="No Data"/>
              <ColorMapEntry color="#feff76" label="1 - 5 persons/sq km"/>
              <ColorMapEntry color="#fcdf50" label="5.1 - 25 persons/sq km"/>
              <ColorMapEntry color="#f7bc2b" label="25.1 - 50 persons/sq km"/>
              <ColorMapEntry color="#d68606" label="50.1 - 100 persons/sq km"/>
              <ColorMapEntry color="#9e4400" label="100.1 - 250 persons/sq km"/>
	      <ColorMapEntry color="#6b0400" label="250+ persons/sq km"/>
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

